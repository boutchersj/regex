let caboose = "The last car on a train is the caboose";
let lastRegex = /caboose$/; // Change this line
let result = lastRegex.test(caboose);

//The anchor '$' character is used to find patterns at the END of strings
//You attach it to the end of the part of the regex that finds the pattern you're looking for
//As with the caret '^' it will return true ONLY IF the pattern is at the very end of the string
